<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>academia.controller.login.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SeguridadFIlter</servlet-name>
  	<display-name>SeguridadFIlter</display-name>
  	<description></description>
  	<servlet-class>academia.controller.login.SeguridadFIlter</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SeguridadFilter</servlet-name>
  	<display-name>SeguridadFilter</display-name>
  	<description></description>
  	<servlet-class>academia.controller.login.SeguridadFilter</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SalirServlet</servlet-name>
  	<display-name>SalirServlet</display-name>
  	<description></description>
  	<servlet-class>academia.controller.login.SalirServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SeguridadFIlter</servlet-name>
  	<url-pattern>/SeguridadFIlter</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SeguridadFilter</servlet-name>
  	<url-pattern>/SeguridadFilter</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SalirServlet</servlet-name>
  	<url-pattern>/SalirServlet</url-pattern>
  </servlet-mapping>
</web-app>
